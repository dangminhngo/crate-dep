    "db:studio": "prisma studio",
    "prebuild": "pnpm generate",
    "predev": "pnpm generate"
  },
  "dependencies": {
    "@prisma/client": "latest"
  },
  "devDependencies": {
    "@types/node": "^18.16.3",
    "prisma": "latest",
    "tsconfig": "workspace:*",
    "tsup": "^6.7.0",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4"
  }
